using System;
using System.Collections.Generic;
using System.Linq;

namespace ConsoleApp171
{
    internal class Program
    {
        static void Main(string[] args)
        {

            string str1 = Console.ReadLine();
            string str2 = Console.ReadLine();

            if(str2.Contains(str1))
            {
                Console.WriteLine(1);
            }
            else
            {
                Console.WriteLine(0);
            }

        }
    }
}
