using System;
using System.Collections.Generic;
using System.Linq;

namespace ConsoleApp171
{
    internal class Program
    {
        static void Main(string[] args)
        {

            List<int> numbers = new List<int> { 34, 5, 71, 29, 100, 34 };

            int n = int.Parse(Console.ReadLine());  // 정수 n 입력받기 
            int result = 0; //result 초기화

            
            for(int i =0; i < numbers.Count; i++  )
            {
                result += numbers[i];   // result에 numbers 배열에서 i번째 요소를 더하는 것    
                if (result > n)
                {
                    break;
                   
                }
            }

            Console.WriteLine(result);

            
        }
    }
}
