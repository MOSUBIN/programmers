using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp174
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int n = int.Parse(Console.ReadLine());

            for(int i = 0; i < n; i++)
            {
                if(i % 2 == 0)
                {
                    Console.Write("수");
                }

                else
                {
                    Console.Write("박");
                }
            }
        }
    }
    }

